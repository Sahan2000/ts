//  TypeScript has 3 primitive types:
    /*
        number
        string
        boolean
    */

// -------------------------
let msg: string;
msg = "hello";

// -------------------------

let num: number = 10;
let prf: number = 75.5;

// -------------------------

let isAlive: boolean = true;

// -------------------------

//  -- any type -- //
let x: any = 10;
x = 'hello';
x = true;

export{}